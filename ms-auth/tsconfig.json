{
    "compilerOptions": {
        "module": "CommonJS",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "target": "ES2020",
        "noImplicitAny": true,
        "strict": true,
        "moduleResolution": "node",
        "outDir": "./cache"
    },
    "exclude": ["node_modeules"],
    "include": ["./src/**/*.ts"]
}